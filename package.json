{
    "name": "binaryjump",
    "displayName": "Binary Jump",
    "publisher": "vipinbathaw",
    "version": "0.2.1",
    "description": "Binary Jump is a VS Code extension that helps in faster code navigation by allowing you to jump half the distance from current cursor to either direction.",
    "main": "./extension",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "commands": [
            {
                "command": "binaryJump.binaryLeftJump",
                "title": "Binary Left Jump"
            },
            {
                "command": "binaryJump.binaryRightJump",
                "title": "Binary Right Jump"
            },
            {
                "command": "binaryJump.binaryUpJump",
                "title": "Binary Up Jump"
            },
            {
                "command": "binaryJump.binaryDownJump",
                "title": "Binary Down Jump"
            }
        ],
        "keybindings": [
            {
                "command": "binaryJump.binaryLeftJump",
                "key": "ctrl+meta+alt+left",
                "mac": "ctrl+cmd+alt+left",
                "when": "editorTextFocus"
            },
            {
                "command": "binaryJump.binaryRightJump",
                "key": "ctrl+meta+alt+right",
                "mac": "ctrl+cmd+alt+right",
                "when": "editorTextFocus"
            },
            {
                "command": "binaryJump.binaryUpJump",
                "key": "ctrl+meta+alt+up",
                "mac": "ctrl+cmd+alt+up",
                "when": "editorTextFocus"
            },
            {
                "command": "binaryJump.binaryDownJump",
                "key": "ctrl+meta+alt+down",
                "mac": "ctrl+cmd+alt+down",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "test": "node ./node_modules/vscode/bin/test",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "eslint": "^4.11.0",
        "typescript": "^3.1.4",
        "vscode": "^1.1.36"
    },
    "author": "Vipin Bathaw",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/vipinbathaw/vscode-binary-jump.git"
    },
    "icon": "img/logo.png",
    "galleryBanner": {
        "color": "#21272e",
        "theme": "dark"
    }
}
